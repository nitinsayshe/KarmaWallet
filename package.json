{
  "name": "karmawallet-backend-ts",
  "version": "1.0.0",
  "description": "the karma wallet backend",
  "main": "index.ts",
  "scripts": {
    "migrate": "ts-node ./bin/v3_migration.ts",
    "db_sync": "ts-node ./bin/pull_remote_db.ts",
    "reset_migration": "ts-node ./bin/reset_v3_migration.ts && npm run db_sync",
    "clean": "rm -rf dist",
    "copy": "ts-node ./bin/postbuild.ts",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "npm run copy",
    "postinstall": "npm run build",
    "start": "cross-env-shell NODE_ENV=development 'tsc -w & nodemon dist/index.js && npm run copy'",
    "start:prod": "cross-env-shell NODE_ENV=production 'npm run build & node dist/index.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karmawallet/karmawallet-backend-ts.git"
  },
  "author": "@jddalessandro, @swallsy and @iamthe-Wraith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karmawallet/karmawallet-backend-ts/issues"
  },
  "homepage": "https://github.com/karmawallet/karmawallet-backend-ts#readme",
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.1",
    "@types/express-serve-static-core": "4.17.28",
    "@types/ioredis": "4.28.7",
    "@types/json2csv": "5.0.3",
    "@types/lodash": "4.14.178",
    "@types/mongoose-paginate-v2": "1.4.3",
    "@types/node": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "dayjs": "1.10.7",
    "eslint": "8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-unused-imports": "2.0.0",
    "nodemon": "2.0.15",
    "ts-node": "10.4.0"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "7.1.0",
    "api-query-params": "5.3.1",
    "argon2": "0.28.3",
    "aws-sdk": "2.1066.0",
    "axios": "0.25.0",
    "body-parser": "1.19.1",
    "bullmq": "1.73.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "csvtojson": "2.0.10",
    "date-fns": "2.28.0",
    "dotenv": "14.3.2",
    "express": "4.17.1",
    "express-serve-static-core": "0.1.1",
    "handlebars": "4.7.7",
    "helmet": "5.0.2",
    "ioredis": "4.28.3",
    "json2csv": "5.0.6",
    "lodash": "4.17.21",
    "mongoose": "6.1.8",
    "mongoose-paginate-v2": "1.5.0",
    "nanoid": "3.2.0",
    "pino-http": "6.6.0",
    "plaid": "9.10.0",
    "rate-limiter-flexible": "2.3.6",
    "socket.io": "4.4.1",
    "typescript": "4.5.5"
  }
}
