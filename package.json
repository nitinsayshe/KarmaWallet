{
  "name": "karmawallet-backend-ts",
  "version": "1.0.0",
  "description": "the karma wallet backend",
  "main": "index.ts",
  "scripts": {
    "scripts": "ts-node ./bin/scripts.ts",
    "db_sync": "ts-node ./bin/pull_remote_db.ts",
    "reset_migration": "ts-node ./bin/reset_v3_migration.ts && npm run db_sync",
    "clean": "rm -rf dist",
    "copy": "ts-node ./bin/postbuild.ts",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "LOG_LEVEL=off SUPPRESS_JEST_WARNINGS=1 ./node_modules/.bin/jest --detectOpenHandles --forceExit --setupFiles dotenv/config",
    "dockerTest": "docker-compose -f docker-compose.test.yml down; docker-compose -f docker-compose.test.yml up --build --exit-code-from karmawallet-backend-ts-test; docker-compose -f docker-compose.test.yml rm -fsv",
    "postbuild": "npm run copy",
    "postinstall": "npm run build",
    "start": "cross-env-shell NODE_ENV=development 'tsc -w & nodemon dist/index.js && npm run copy'",
    "start:frontend": "cross-env-shell NODE_ENV=development 'tsc -w & nodemon dist/frontend.js && npm run copy'",
    "start:prod": "cross-env-shell NODE_ENV=production 'npm run build & node dist/index.js'",
    "start:prod:frontend": "cross-env-shell NODE_ENV=production 'npm run build & node dist/frontend.js'",
    "deploy:sandbox": "ts-node bin/deploy-sandbox -- --env sandbox",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karmawallet/karmawallet-backend-ts.git"
  },
  "author": "@jddalessandro, @swallsy, @adelgado0723, @SenciZ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karmawallet/karmawallet-backend-ts/issues"
  },
  "homepage": "https://github.com/karmawallet/karmawallet-backend-ts#readme",
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@types/compression": "1.7.2",
